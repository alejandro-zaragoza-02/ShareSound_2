<?xml version="1.0" encoding="utf-8" ?>
		<hibernate-mapping xmlns="urn:nhibernate-mapping-2.2"
		                   assembly="ShareSound_2GenNHibernate"
		                   namespace="ShareSound_2GenNHibernate.EN.ShareSound_2">
		    
		    
			
				<class name="ListaEN" table="Lista"> 
				  	
						<id name="Id" column="idLista" type="integer"> 
							<generator class="hilo"/>
						</id>
			
					
					<property name="Titulo" type="string"> 
						<column name="titulo"  not-null="true"/>
					</property>
					<property name="Descripcion" type="string"> 
						<column name="descripcion"  not-null="true"/>
					</property>
					<property name="Imagen" type="string"> 
						<column name="imagen"  not-null="true"/>
					</property>
					<property name="Publico" type="boolean"> 
						<column name="publico"  not-null="true"/>
					</property>
					<property name="Fecha" type="date"> 
						<column name="fecha"  not-null="true"/>
					</property>
			
			
			
			
			
			
			
					
					
					
					
					
					
		
		 </class>	
		  
		  <query name="ListaENReadAllHQL">from ListaEN</query>
		</hibernate-mapping>
	